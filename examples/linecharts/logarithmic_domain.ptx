width: 1200px;
height: 480px;

plot {
  x: csv('tests/testdata/log_example.csv', x);
  y: csv('tests/testdata/log_example.csv', y);

  axis-x: log;
  axis-x-min: 0;
  axis-x-max: 5;

  axis-y: log;
  axis-y-min: 0;
  axis-y-max: 10000;

  axis-top: off;
  axis-right: off;

  layer {
    type: lines;
    title: "To the Moon";
  }

  legend {
    position: top left inside;
  }
}
