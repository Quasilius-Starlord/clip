width: 1200px;
height: 480px;

plot {
  x: csv('tests/testdata/log_example.csv', x);
  y: csv('tests/testdata/log_example.csv', y);

  scale-x: log;
  scale-x-min: 0;
  scale-x-max: 5;

  scale-y: log;
  scale-y-min: 0;
  scale-y-max: 10000;

  axis-top: off;
  axis-right: off;

  layer {
    type: lines;
    title: "To the Moon";
  }

  legend {
    position: top left inside;
  }
}
