width: 1200px;
height: 480px;

scale-y-min: 0;
scale-y-max: 200;

points {
  xs: csv('tests/testdata/measurement.csv', time);
  ys: csv('tests/testdata/measurement.csv', value2);
  sizes: 2pt, 3pt;
  colors: #06c, #c06;
}

axis {
  position: bottom;
  format: datetime("%H:%M:%S");
  layout: linear(3600, align 3600);
}

axis {
  position: left;
  layout: linear(20);
}

axis {
  position: right;
  layout: linear(10);
}

axis {
  position: top;
  format: datetime("%H:%M:%S");
}

