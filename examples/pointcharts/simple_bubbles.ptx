width: 1200px;
height: 480px;

scale-y-max: 60;
scale-y-min: -40;
scale-x-min: -60;
scale-x-max: 60;

points {
  xs: csv('tests/testdata/point_example.csv', x);
  ys: csv('tests/testdata/point_example.csv', y);
  sizes: csv('tests/testdata/point_example.csv', z);
  color: #06c, #c06;
  size: 2pt;
}

axis {
  position: bottom;
  layout: linear(10);
}

axis {
  position: top;
  layout: linear(10);
}
