(chart/linechart
    data-x (csv "tests/testdata/measurement.csv" time)
    data-y (csv "tests/testdata/measurement.csv" value1)
    axis-left (
        labels (scientific)
        title "Value"
        title-font-size 14pt)
    axis-right (
        labels (scientific)
        title "Value"
        title-rotate 180
        title-font-size 14pt)
    axis-top (
        labels (datetime "%H:%M:%S")
        title "Time"
        title-font-size 14pt)
    axis-bottom (
        labels (datetime "%H:%M:%S")
        title "Time"
        title-font-size 14pt))
