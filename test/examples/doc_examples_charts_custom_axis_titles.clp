(chart/linechart
    data-x (csv "test/testdata/measurement.csv" time)
    data-y (csv "test/testdata/measurement.csv" value1)
    axis-left (
        label-format (scientific)
        title "Value"
        title-font-size 14pt)
    axis-right (
        label-format (scientific)
        title "Value"
        title-rotate 90
        title-font-size 14pt)
    axis-top (
        label-format (datetime "%H:%M:%S")
        title "Time"
        title-font-size 14pt)
    axis-bottom (
        label-format (datetime "%H:%M:%S")
        title "Time"
        title-font-size 14pt))
