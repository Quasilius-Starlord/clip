(set height 300px)
(set width 960px)

(plot
    axes (left)
    limit-x (0 24)
    scale-y (categorical (csv "tests/testdata/gdp_per_capita_2010.csv" country))
    bars (
      data-x (csv "tests/testdata/gdp_per_capita_2010.csv" gdp)
      data-y (csv "tests/testdata/gdp_per_capita_2010.csv" country)
      labels (csv "tests/testdata/gdp_per_capita_2010.csv" gdp_label)
      direction horizontal
      bar-width (1.4em)
      color #4c7bb0))
