(chart/layout
    axes (top right bottom left)
    limit-x (0 13)
    scale-y-padding 5
    axis-x-ticks (linear 1 1 12)
    axis-x-ticks (linear 1 1 12)
    axis-x-labels (custom "Jan" "Feb" "Mar" "Apr" "May" "Jun" "Jul" "Aug" "Sep" "Oct" "Nov" "Dec")
    bars (
      data-x (csv "tests/testdata/city_temperatures.csv" month)
      data-y (csv "tests/testdata/city_temperatures.csv" temp_moscow)
      bar-width (2em)
      color (#666)))
