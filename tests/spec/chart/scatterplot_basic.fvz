(chart/scatterplot
    data-x (csv tests/testdata/gauss2d.csv x)
    data-y (csv tests/testdata/gauss2d.csv y)
    axes (bottom left)
    grid (color #fff)
    legend (items ("Random Data"))
    axis-y-ticks (subdivide 5)
    background #eee
    border none
    limit-x-min 0
    limit-x-max 400
    limit-y-min 0
    limit-y-max 200)
