(chart/layout
    axes (left)
    limit-x (0 24)
    limit-y (0 10)
    ticks-y (discrete)
    axis-y-format (custom (csv "tests/testdata/gdp_per_capita_2010.csv" country))
    bars (
      data-x (csv "tests/testdata/gdp_per_capita_2010.csv" gdp)
      data-y (csv "tests/testdata/gdp_per_capita_2010.csv" rank)
