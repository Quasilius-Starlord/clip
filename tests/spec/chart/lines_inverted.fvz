(set height 480px)
(set width 1200px)

(chart/linechart
    data-x (csv "tests/testdata/measurement.csv" time)
    data-y (csv "tests/testdata/measurement.csv" value2)
    color (#06c)
    axes (bottom left)
    scale-y (inverted)
    limit-y (0 140)
    axis-x-ticks (subdivide 6)
    axis-y-ticks (linear 20 0)
    axis-x-labels (datetime "%H:%M:%S"))
